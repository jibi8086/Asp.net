            <ul>
            </ul>
            <ul id="nav-mobile" class="right hide-on-med-and-down">
                <li>
                </li>
                <li> <a id="uname" class="dropdown-button  #1565c0 blue darken" href="#" data-activates='dropdown1'><i class="small material-icons left">person</i></a></li>
                <ul id='dropdown1' class='dropdown-content'>
                <li><a href="#!" onclick="location.href='@Url.Action("SessionClear","UserLogin")'"><i class="material-icons left">settings_power</i> LogOut</a></li>
                </ul>
           </ul>
        </div>
    </nav>

